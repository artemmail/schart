{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-material-login-template": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-material-login-template",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"          ,
{
                "glob": "**/*",
                "input": "src/assets/tinymce",
                "output": "/tinymce/"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.css"
            ]

          },
          "configurations": {
    "production": {
      "budgets": [
        {
          "type": "initial",
          "maximumWarning": "5000kb",
          "maximumError": "10mb"
        },
        {
          "type": "anyComponentStyle",
          "maximumWarning": "200kb",
          "maximumError": "400kb"
        }
      ],
      "optimization": {
        "fonts": {
          "inline": false
        }
      },
      "outputHashing": "all"
    },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "angular-material-login-template:build:production"
            },
            "development": {
              "buildTarget": "angular-material-login-template:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "angular-material-login-template:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
  {
    "glob": "**/*",
    "input": "src/assets/tinymce",
    "output": "/assets/tinymce/"
  }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "stockchart-treemap": {
      "projectType": "library",
      "root": "projects/stockchart-treemap",
      "sourceRoot": "projects/stockchart-treemap/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/stockchart-treemap/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/stockchart-treemap/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/stockchart-treemap/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/stockchart-treemap/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "52def61b-4111-4cc6-a1c3-af91941c4266"
  }
}
